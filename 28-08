    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // jTextField1.setText("Deu certo!");
        jTextField1.getText();
        jTextField2.getText();
        if (jTextField1.getText().equals("root") && jTextField2.getText().equals("123")) {
            jLabel3.setText("Dados corretos!");
        } else {
            jLabel3.setText("Dados incorretos!");
        }

    }                                        

